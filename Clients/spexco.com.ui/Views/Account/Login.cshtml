    <script type="text/javascript">
        $(document).ready(function () {
            toastr.options = {
                closeButton: true,
                debug: true,
                progressBar: true,
                positionClass: 'toast-top-right',
                timeOut: 5000,
                onclick: null
            };
        });
        var btn = null;
        function OnBegin() {
            if (btn != null) {
                btn.ladda('start');
            }
        }
        function OnSuccess(data) {
            if (btn != null) {
                btn.ladda('stop');
            }
        }
    </script>
</head>
<body class="gray-bg">
    <form action="/Account/Login" class="m-t" data-ajax="true" data-ajax-begin="OnBegin" data-ajax-complete="OnSuccess" data-ajax-failure="OnSuccess" data-ajax-method="POST" data-ajax-mode="replace" data-ajax-success="OnSuccess" data-ajax-update="#frm_login" id="form0" method="post">
        <div id="loginbox" class="middle-box text-center loginscreen  animated fadeInDown">

            <div id="frm_login" class="login">
                @await Html.PartialAsync("~/Views/Account/LoginForm.cshtml")
            </div>
        </div>
    </form>



</body>
